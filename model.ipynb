    "attendance_mode = df['Attendance'].mode()[0]\n",
    "df['Attendance'] = df['Attendance'].replace('3', attendance_mode)\n",
    "\n",
    "# Convert Scholarship percentages\n",
    "df['Scholarship'] = df['Scholarship'].replace({'None': '0%', '100': '100%'})  # Handle edge cases\n",
    "df['Scholarship'] = df['Scholarship'].str.rstrip('%').astype(float) / 100.0\n",
    "\n",
    "# Convert age ranges to numerical\n",
    "def convert_age(age):\n",
    "    if '-' in age:\n",
    "        low, high = map(int, age.split('-'))\n",
