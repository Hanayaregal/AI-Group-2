    "# Data Cleaning\n",
    "# List of Yes/No columns to clean\n",
    "yes_no_cols = ['Additional_Work', 'Sports_activity', 'Reading', 'Notes', \n",
    "              'Listening_in_Class', 'Project_work']\n",
    "\n",
    "# Clean all Yes/No columns\n",
    "for col in yes_no_cols:\n",
    "    # Replace numerical values with mode\n",
    "    mode_value = df[col].mode()[0]\n",
    "    df[col] = df[col].replace({'6': mode_value, '3': mode_value})  # Handle any numerical contaminants\n",
